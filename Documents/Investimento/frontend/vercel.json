{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/trades",
      "dest": "/trades.html"
    },
    {
      "src": "/settings",
      "dest": "/settings.html"
    },
    {
      "src": "/ai-models",
      "dest": "/ai-models.html"
    },
    {
      "src": "/docs",
      "dest": "/docs.html"
    }
  ]
}
